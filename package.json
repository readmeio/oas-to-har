{
  "name": "@readme/oas-to-har",
  "description": "Utility to transform an OAS operation into a HAR representation",
  "version": "21.0.4",
  "author": "Jon Ursenbach <jon@ursenba.ch>",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "git://github.com/readmeio/oas-to-har.git"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    },
    "./targets": {
      "types": "./dist/targets.d.ts",
      "require": "./dist/targets.js",
      "import": "./dist/targets.mjs"
    },
    "./package.json": "./package.json"
  },
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "sideEffects": false,
  "engines": {
    "node": ">=16"
  },
  "scripts": {
    "build": "tsup",
    "lint": "eslint .",
    "prebuild": "rm -rf dist/",
    "prepack": "npm run build",
    "prepare": "husky install",
    "pretest": "npm run lint",
    "prettier": "prettier --list-different --write \"./**/**.{js,ts}\"",
    "test": "vitest --coverage"
  },
  "dependencies": {
    "@readme/data-urls": "^1.0.1",
    "@readme/oas-extensions": "^18.0.0",
    "lodash": "^4.17.21",
    "oas": "^21.0.2",
    "qs": "^6.11.2",
    "remove-undefined-objects": "^3.0.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.7.1",
    "@commitlint/config-conventional": "^17.7.0",
    "@readme/eslint-config": "^12.2.0",
    "@readme/oas-examples": "^5.12.0",
    "@types/har-format": "^1.2.12",
    "@types/lodash": "^4.14.197",
    "@types/qs": "^6.9.8",
    "@vitest/coverage-v8": "^0.34.1",
    "eslint": "^8.48.0",
    "har-validator": "^5.1.5",
    "husky": "^8.0.3",
    "jest-expect-har": "^6.0.0",
    "prettier": "^3.0.3",
    "tsup": "^7.2.0",
    "type-fest": "^4.3.1",
    "typescript": "^5.2.2",
    "vitest": "^0.34.1"
  },
  "browserslist": [
    "last 2 versions"
  ],
  "prettier": "@readme/eslint-config/prettier",
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ],
    "rules": {
      "body-max-line-length": [
        0,
        "always",
        "Infinity"
      ],
      "footer-max-line-length": [
        0,
        "always",
        "Infinity"
      ]
    }
  }
}
