{
  "name": "@readme/oas-to-har",
  "description": "Utility to transform an OAS operation into a HAR representation",
  "version": "21.0.0",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "author": "Jon Ursenbach <jon@ursenba.ch>",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "git://github.com/readmeio/oas-to-har.git"
  },
  "engines": {
    "node": ">=16"
  },
  "scripts": {
    "build": "tsc",
    "lint": "eslint .",
    "prebuild": "rm -rf dist/",
    "prepack": "npm run build",
    "prepare": "husky install",
    "pretest": "npm run lint",
    "prettier": "prettier --list-different --write \"./**/**.{js,ts}\"",
    "release": "npx conventional-changelog-cli -i CHANGELOG.md -s && git add CHANGELOG.md",
    "test": "jest --coverage"
  },
  "dependencies": {
    "@readme/data-urls": "^1.0.1",
    "@readme/oas-extensions": "^17.0.1",
    "oas": "^20.8.4",
    "qs": "^6.11.2",
    "remove-undefined-objects": "^3.0.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.6.6",
    "@commitlint/config-conventional": "^17.6.6",
    "@readme/eslint-config": "^10.6.0",
    "@readme/oas-examples": "^5.11.1",
    "@types/har-format": "^1.2.11",
    "@types/jest": "^29.5.2",
    "@types/qs": "^6.9.7",
    "eslint": "^8.43.0",
    "har-validator": "^5.1.5",
    "husky": "^8.0.3",
    "jest": "^29.5.0",
    "jest-expect-har": "^6.0.0",
    "prettier": "^2.8.8",
    "ts-jest": "^29.1.0",
    "type-fest": "^3.12.0",
    "typescript": "^5.1.3"
  },
  "browserslist": [
    "last 2 versions"
  ],
  "prettier": "@readme/eslint-config/prettier",
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ],
    "rules": {
      "body-max-line-length": [
        0,
        "always",
        "Infinity"
      ],
      "footer-max-line-length": [
        0,
        "always",
        "Infinity"
      ]
    }
  }
}
