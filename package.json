{
  "name": "@readme/oas-to-har",
  "description": "Utility to transform an OAS operation into a HAR representation",
  "version": "13.7.0",
  "main": "src/index.js",
  "author": "Jon Ursenbach <jon@ursenba.ch>",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "git://github.com/readmeio/oas-to-har.git"
  },
  "engines": {
    "node": "^12 || ^14 || ^16",
    "npm": "^7"
  },
  "scripts": {
    "lint": "eslint .",
    "prepare": "husky install",
    "pretest": "npm run lint",
    "prettier": "prettier --list-different --write \"./**/**.{js,jsx}\"",
    "release": "npx conventional-changelog-cli -i CHANGELOG.md -s && git add CHANGELOG.md",
    "test": "jest --coverage"
  },
  "dependencies": {
    "@readme/oas-extensions": "^13.6.0",
    "oas": "^14.0.0",
    "parse-data-url": "^4.0.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^13.1.0",
    "@commitlint/config-conventional": "^13.1.0",
    "@readme/eslint-config": "^6.0.0",
    "@readme/oas-to-snippet": "^14.0.0",
    "datauri": "^4.1.0",
    "eslint": "^7.32.0",
    "husky": "^7.0.1",
    "jest": "^27.0.6",
    "jest-expect-har": "^2.0.2",
    "prettier": "^2.0.1"
  },
  "prettier": "@readme/eslint-config/prettier",
  "jest": {
    "setupFilesAfterEnv": [
      "jest-expect-har"
    ]
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ],
    "rules": {
      "body-max-line-length": [
        0,
        "always",
        "Infinity"
      ],
      "footer-max-line-length": [
        0,
        "always",
        "Infinity"
      ]
    }
  }
}
