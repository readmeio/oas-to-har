{
  "name": "@readme/oas-to-har",
  "description": "Utility to transform an OAS operation into a HAR representation",
  "version": "20.1.1",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "author": "Jon Ursenbach <jon@ursenba.ch>",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "git://github.com/readmeio/oas-to-har.git"
  },
  "engines": {
    "node": ">=14"
  },
  "scripts": {
    "build": "tsc",
    "lint": "eslint .",
    "prebuild": "rm -rf dist/",
    "prepack": "npm run build",
    "prepare": "husky install",
    "pretest": "npm run lint",
    "prettier": "prettier --list-different --write \"./**/**.{js,ts}\"",
    "release": "npx conventional-changelog-cli -i CHANGELOG.md -s && git add CHANGELOG.md",
    "test": "nyc mocha \"test/**/*.test.ts\"",
    "test:browser": "karma start --single-run",
    "test:browser:chrome": "karma start --browsers=Chrome --single-run=false",
    "test:browser:debug": "karma start --single-run=false"
  },
  "dependencies": {
    "@readme/data-urls": "^1.0.1",
    "@readme/oas-extensions": "^17.0.1",
    "oas": "^20.5.3",
    "qs": "^6.11.1",
    "remove-undefined-objects": "^2.0.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.5.1",
    "@commitlint/config-conventional": "^17.4.4",
    "@jsdevtools/host-environment": "^2.1.2",
    "@jsdevtools/karma-config": "^3.1.7",
    "@readme/eslint-config": "^10.5.2",
    "@readme/oas-examples": "^5.10.0",
    "@types/chai": "^4.3.1",
    "@types/har-format": "^1.2.10",
    "@types/mocha": "^10.0.0",
    "@types/qs": "^6.9.7",
    "chai": "^4.3.6",
    "eslint": "^8.37.0",
    "har-validator": "^5.1.5",
    "husky": "^8.0.3",
    "mocha": "^10.2.0",
    "nyc": "^15.1.0",
    "prettier": "^2.8.7",
    "ts-loader": "^8.4.0",
    "ts-node": "^10.9.1",
    "type-fest": "^3.7.2",
    "typescript": "^5.0.3",
    "webpack": "^4.46.0"
  },
  "browserslist": [
    "last 2 versions"
  ],
  "prettier": "@readme/eslint-config/prettier",
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ],
    "rules": {
      "body-max-line-length": [
        0,
        "always",
        "Infinity"
      ],
      "footer-max-line-length": [
        0,
        "always",
        "Infinity"
      ]
    }
  }
}
