{
  "name": "@readme/oas-to-har",
  "description": "Utility to transform an OAS operation into a HAR representation",
  "version": "23.0.5",
  "author": "Jon Ursenbach <jon@ursenba.ch>",
  "license": "ISC",
  "sideEffects": false,
  "type": "module",
  "exports": {
    ".": {
      "require": "./dist/index.cjs",
      "import": "./dist/index.js"
    },
    "./lib/configure-security": {
      "require": "./dist/lib/configure-security.cjs",
      "import": "./dist/lib/configure-security.js"
    },
    "./lib/types": {
      "require": "./dist/lib/types.cjs",
      "import": "./dist/lib/types.js"
    },
    "./package.json": "./package.json"
  },
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.cts",
  "engines": {
    "node": ">=18"
  },
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/readmeio/oas-to-har.git"
  },
  "scripts": {
    "build": "tsup",
    "lint": "eslint .",
    "prebuild": "rm -rf dist/",
    "prepack": "npm run build",
    "prepare": "husky install",
    "pretest": "npm run lint",
    "prettier": "prettier --list-different --write \"./**/**.{js,ts}\"",
    "test": "vitest --coverage"
  },
  "dependencies": {
    "@readme/data-urls": "^3.0.0",
    "@readme/oas-extensions": "^20.0.0",
    "lodash": "^4.17.21",
    "oas": "^21.1.1",
    "qs": "^6.11.2",
    "remove-undefined-objects": "^4.0.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.7.1",
    "@commitlint/config-conventional": "^17.7.0",
    "@readme/eslint-config": "^12.2.1",
    "@readme/oas-examples": "^5.12.0",
    "@types/har-format": "^1.2.12",
    "@types/lodash": "^4.14.198",
    "@types/qs": "^6.9.8",
    "@vitest/coverage-v8": "^0.34.4",
    "eslint": "^8.49.0",
    "husky": "^8.0.3",
    "jest-expect-har": "^6.0.0",
    "prettier": "^3.0.3",
    "tsup": "^7.2.0",
    "type-fest": "^4.3.1",
    "typescript": "^5.2.2",
    "vitest": "^0.34.4"
  },
  "browserslist": [
    "last 2 versions"
  ],
  "prettier": "@readme/eslint-config/prettier",
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ],
    "rules": {
      "body-max-line-length": [
        0,
        "always",
        "Infinity"
      ],
      "footer-max-line-length": [
        0,
        "always",
        "Infinity"
      ]
    }
  }
}
